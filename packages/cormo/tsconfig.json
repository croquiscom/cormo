{
  "compilerOptions": {
    "target": "es2021",
    "noImplicitAny": true,
    "noImplicitThis": true,
    "strictNullChecks": true,
    "strict": true,
    "stripInternal": true,
    "declaration": true,
    "module": "CommonJS",
    "skipLibCheck": true,
    "experimentalDecorators": true,
    "esModuleInterop": true,
    "allowJs": true
  }
}
