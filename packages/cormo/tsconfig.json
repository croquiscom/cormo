{
  "compilerOptions": {
    "target": "ES2021",
    "noImplicitAny": true,
    "noImplicitThis": true,
    "strictNullChecks": true,
    "strict": true,
    "stripInternal": true,
    "declaration": true,
    "module": "NodeNext",
    "moduleResolution": "NodeNext",
    "skipLibCheck": true,
    "experimentalDecorators": true,
    "esModuleInterop": true,
    "allowJs": true,
    "lib": ["ES2021"]
  }
}
