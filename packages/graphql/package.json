{
  "name": "cormo-graphql",
  "version": "0.3.1",
  "description": "GraphQL support for CORMO",
  "main": "./lib",
  "types": "./lib/index.d.ts",
  "scripts": {
    "prepublishOnly": "npm run lint && npm run build",
    "build": "rimraf lib && tsc -p src/tsconfig.build.json",
    "lint": "tslint -p tsconfig.json",
    "test:type": "tsc --noEmit",
    "test:unit": "mocha --require ts-node/register/transpile-only ./test/*/*.ts",
    "test": "npm run test:type && npm run lint && npm run test:unit --",
    "test:debug": "npm run test:unit -- --inspect-brk=30000"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/croquiscom/cormo.git"
  },
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/croquiscom/cormo/issues"
  },
  "homepage": "https://github.com/croquiscom/cormo",
  "dependencies": {
    "@croquiscom/crary-graphql": "^0.3.7",
    "cormo": "^0.13.1",
    "graphql": "^14.6.0",
    "lodash": "^4.17.15"
  },
  "devDependencies": {
    "@types/chai": "^4.2.9",
    "@types/graphql": "^14.5.0",
    "@types/lodash": "^4.14.149",
    "@types/mocha": "^7.0.1",
    "@types/node": "^13.7.4",
    "@types/sinon": "^7.5.1",
    "chai": "^4.2.0",
    "mocha": "^7.0.1",
    "mysql": "^2.18.1",
    "rimraf": "^3.0.2",
    "sinon": "^9.0.0",
    "ts-node": "^8.6.2",
    "tslint": "^6.0.0",
    "typescript": "^3.8.2"
  }
}
