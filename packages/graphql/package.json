{
  "name": "cormo-graphql",
  "version": "0.1.0",
  "description": "GraphQL support for CORMO",
  "main": "./lib",
  "types": "./lib/index.d.ts",
  "scripts": {
    "prepublishOnly": "npm run lint && npm run build",
    "build": "rimraf lib && tsc -p src/tsconfig.build.json",
    "lint": "tslint -p tsconfig.json",
    "test:type": "tsc --noEmit",
    "test:unit": "mocha --require ts-node/register/transpile-only ./test/*/*.ts",
    "test": "npm run test:type && npm run lint && npm run test:unit --"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/croquiscom/cormo.git"
  },
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/croquiscom/cormo/issues"
  },
  "homepage": "https://github.com/croquiscom/cormo",
  "dependencies": {
    "cormo": "^0.12.2",
    "graphql": "^14.4.2"
  },
  "devDependencies": {
    "@types/chai": "^4.1.7",
    "@types/graphql": "^14.2.2",
    "@types/mocha": "^5.2.7",
    "chai": "^4.2.0",
    "mocha": "^6.2.0",
    "rimraf": "^2.6.3",
    "ts-node": "^8.3.0",
    "tslint": "^5.18.0",
    "typescript": "^3.5.3"
  }
}
