{
  "name": "cormo",
  "description": "ORM framework for Node.js",
  "version": "0.10.0",
  "keywords": [
    "orm",
    "mongodb",
    "mysql",
    "sqlite3",
    "postgresql"
  ],
  "homepage": "https://croquiscom.github.com/cormo",
  "bugs": {
    "url": "https://github.com/croquiscom/cormo/issues"
  },
  "author": "Sangmin Yoon <sangmin.yoon@croquis.com>",
  "license": "MIT",
  "bin": {
    "cormo": "./bin/cormo"
  },
  "scripts": {
    "prepublishOnly": "npm run lint && npm run build",
    "build": "rimraf lib && tsc -p src/tsconfig.build.json",
    "lint": "tslint -p tsconfig.json",
    "doc": "node node_modules/.bin/crojsdoc",
    "test": "NODE_ENV=test mocha --require coffeescript/register --require ts-node/register --exit 'test/*.{js,ts,coffee}'"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/croquiscom/cormo.git"
  },
  "types": "./lib/index.d.ts",
  "dependencies": {
    "commander": "^2.18.0",
    "inflected": "^2.0.4",
    "lodash": "^4.17.11",
    "toposort-class": "^1.0.1"
  },
  "devDependencies": {
    "@types/chai": "^4.1.5",
    "@types/inflected": "^1.1.29",
    "@types/lodash": "^4.14.116",
    "@types/mocha": "^5.2.5",
    "@types/node": "^8.10.29",
    "benchmark": "^2.1.4",
    "chai": "^4.1.2",
    "coffee-coverage": "^3.0.0",
    "coffeescript": "^2.3.2",
    "crojsdoc": "^0.8.20",
    "microtime": "^2.1.8",
    "mocha": "^5.2.0",
    "mongodb": "^3.1.6",
    "mysql": "^2.16.0",
    "pg": "^7.4.3",
    "pg-query-stream": "^1.0.0",
    "redis": "^2.7.1",
    "rimraf": "^2.6.2",
    "sqlite3": "^4.0.2",
    "ts-node": "^7.0.1",
    "tslint": "^5.11.0",
    "typescript": "^3.0.3"
  }
}
